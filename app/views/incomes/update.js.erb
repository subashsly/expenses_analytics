$("ul.errors").html("")
<% if @income.errors.any? %>
	<% @income.errors.full_messages.each do |msg| %>
		$("ul.errors").append($("<li />").html("<%= msg.html_safe %>"))
	<% end %>
<% else %>
	$('#income-<%= @income.id %>').replaceWith('<%= j render(@income) %>') 
	$("#income-modal").modal("hide")
<% end %>
$("ul.errors").html("")
<% if @contact.errors.any? %>
	<% @contact.errors.full_messages.each do |msg| %>
		$("ul.errors").append($("<li />").html("<%= msg.html_safe %>"))
	<% end %>
<% else %>
	 if ($("#contact")) {
	 	$("#contact").append("<%= j render @contact%>")
		$("#contact-modal").modal("hide")
	 }else{
	 	window.location.href="http://java67.blogspot.com";
	 }
	
	
	
<% end %>
<tr id="contact-<%= contact.id%>">
	<td><%= contact.name%></td>
	<td><%= contact.phone_no %></td>
	<td><%= contact.address %></td>
	<td><%= link_to "Edit", edit_contact_path(contact), 
					remote: true, class: 'btn btn-default' %></td>
	<td><%= link_to "Delete", contact_delete_path(contact), remote: true, class: 'btn btn-danger' %></td>
</tr>